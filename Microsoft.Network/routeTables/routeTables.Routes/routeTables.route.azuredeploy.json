{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "fw-internal-ip":{
            "type": "string"
        },
        "RT-rda-name":{
            "type": "string"
        },
        "RT-rdh-name":{
            "type": "string"
        },
        "RT-data-name":{
            "type": "string"
        },
        "RT-web-name":{
            "type": "string"
        },
        "RT-core-name":{
            "type": "string"
        },
        "RT-test-name":{
            "type": "string"
        },
        "RT-gateway-name":{
            "type": "string"
        },
        "RT-app-name":{
            "type": "string"
        },
        "RT-mgmt-name":{
            "type": "string"
        },
        "vNetPrefix":{
            "type": "string"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Network/routeTables/routes",
            "apiVersion": "2018-12-01",
            "name": "[concat(parameters('RT-rda-name'),'/Default')]",
            "properties": {
                "addressPrefix": "0.0.0.0/0",
                "nextHopType": "VirtualAppliance",
                "nextHopIpAddress": "[parameters('fw-internal-ip')]"
            }
        },
        {
            "type": "Microsoft.Network/routeTables/routes",
            "apiVersion": "2018-12-01",
            "name": "[concat(parameters('RT-rdh-name'),'/Default')]",
            "properties": {
                "addressPrefix": "0.0.0.0/0",
                "nextHopType": "VirtualAppliance",
                "nextHopIpAddress": "[parameters('fw-internal-ip')]"
            }
        },
        {
            "type": "Microsoft.Network/routeTables/routes",
            "apiVersion": "2018-12-01",
            "name": "[concat(parameters('RT-data-name'),'/Default')]",
            "properties": {
                "addressPrefix": "0.0.0.0/0",
                "nextHopType": "VirtualAppliance",
                "nextHopIpAddress": "[parameters('fw-internal-ip')]"
            }
        },
        {
            "type": "Microsoft.Network/routeTables/routes",
            "apiVersion": "2018-12-01",
            "name": "[concat(parameters('RT-web-name'),'/Default')]",
            "properties": {
                "addressPrefix": "0.0.0.0/0",
                "nextHopType": "VirtualAppliance",
                "nextHopIpAddress": "[parameters('fw-internal-ip')]"
            }
        },
        {
            "type": "Microsoft.Network/routeTables/routes",
            "apiVersion": "2018-12-01",
            "name": "[concat(parameters('RT-core-name'),'/Default')]",
            "properties": {
                "addressPrefix": "0.0.0.0/0",
                "nextHopType": "VirtualAppliance",
                "nextHopIpAddress": "[parameters('fw-internal-ip')]"
            }
        },
        {
            "type": "Microsoft.Network/routeTables/routes",
            "apiVersion": "2018-12-01",
            "name": "[concat(parameters('RT-test-name'),'/Default')]",
            "properties": {
                "addressPrefix": "0.0.0.0/0",
                "nextHopType": "VirtualAppliance",
                "nextHopIpAddress": "[parameters('fw-internal-ip')]"
            }
        },
        {
            "type": "Microsoft.Network/routeTables/routes",
            "apiVersion": "2018-12-01",
            "name": "[concat(parameters('RT-app-name'),'/Default')]",
            "properties": {
                "addressPrefix": "0.0.0.0/0",
                "nextHopType": "VirtualAppliance",
                "nextHopIpAddress": "[parameters('fw-internal-ip')]"
            }
        },
        {
            "type": "Microsoft.Network/routeTables/routes",
            "apiVersion": "2018-12-01",
            "name": "[concat(parameters('RT-gateway-name'),'/Default')]",
            "properties": {
                "addressPrefix": "0.0.0.0/0",
                "nextHopType": "VirtualAppliance",
                "nextHopIpAddress": "[parameters('fw-internal-ip')]"
            }
        },
        {
            "type": "Microsoft.Network/routeTables/routes",
            "apiVersion": "2018-12-01",
            "name": "[concat(parameters('RT-mgmt-name'),'/Default')]",
            "properties": {
                "addressPrefix": "0.0.0.0/0",
                "nextHopType": "VirtualAppliance",
                "nextHopIpAddress": "[parameters('fw-internal-ip')]"
            }
        },
        {
            "type": "Microsoft.Network/routeTables/routes",
            "apiVersion": "2018-12-01",
            "name": "[concat(parameters('RT-gateway-name'),'/Default')]",
            "properties": {
                "addressPrefix": "[parameters('vNetPrefix')]",
                "nextHopType": "VirtualAppliance",
                "nextHopIpAddress": "[parameters('fw-internal-ip')]"
            }
        }
    ],
    "outputs": {}
}