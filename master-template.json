{
    "$schema": "https://schema.management.azure.com/schemas/2018-05-01/subscriptionDeploymentTemplate.json#",
    "contentVersion": "1.0.0.1",
    "parameters": {},
    "variables": {
      "deploylocation":"uksouth",
      "vnetRG":"eoe-prod-vnet-RG",
      "MgmtRG":"eoe-prod-mgmt-RG",
      "sharedServicesRG": "eoe-prod-shared-RG"
    },  
    "resources": [
    {
      "type": "Microsoft.Resources/resourceGroups",
      "apiVersion": "2018-05-01",
      "location": "[variables('deploylocation')]",
      "name": "[toLower(variables('vnetRG'))]",
      "properties": {}
    },
    {
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2018-05-01",
      "name": "Deployt-RT",
      "resourceGroup": "[variables('vnetRG')]",
      "dependsOn": [
        "[resourceId('Microsoft.Resources/resourceGroups/',variables('vnetRG'))]"
      ],
      "properties": {
        "mode": "Incremental",
        "templateLink": {
            "uri":"https://raw.githubusercontent.com/tgiorgio/eoe-grs/master/Microsoft.Network/routeTables/routeTables.azuredeploy.json",
            "contentVersion":"1.0.0.0"
          },
        "parametersLink": {
            "uri":"https://raw.githubusercontent.com/tgiorgio/eoe-grs/master/EOE-GRS/eoe.routeTables.parameters.json",
            "contentVersion":"1.0.0.0"
          }
      }
    },
    {
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2018-05-01",
      "name": "Deploy-NSG",
      "resourceGroup": "[variables('vnetRG')]",
      "dependsOn": [
        "[resourceId('Microsoft.Resources/resourceGroups/',variables('vnetRG'))]"
      ],
      "properties": {
        "mode": "Incremental",
        "templateLink": {
            "uri":"https://raw.githubusercontent.com/tgiorgio/eoe-grs/master/Microsoft.Network/networkSecurityGroups/networkSecurityGroups.azuredeploy.json",
            "contentVersion":"1.0.0.0"
          },
        "parametersLink": {
            "uri":"https://raw.githubusercontent.com/tgiorgio/eoe-grs/master/EOE-GRS/eoe.networkSecurityGroups.parameters.json",
            "contentVersion":"1.0.0.0"
          }
      }
    },
    {
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2018-05-01",
      "name": "Deployt-VNET",
      "resourceGroup": "[variables('vnetRG')]",
      "dependsOn": [
        "[resourceId('Microsoft.Resources/resourceGroups/',variables('vnetRG'))]",
        "Deploy-NSG",
        "Deployt-RT"
      ],
      "properties": {
        "mode": "Incremental",
        "templateLink": {
            "uri":"https://raw.githubusercontent.com/tgiorgio/eoe-grs/master/Microsoft.Network/virtualNetworks/virtualNetworks.azuredeploy.json",
            "contentVersion":"1.0.0.0"
          },
        "parametersLink": {
            "uri":"https://raw.githubusercontent.com/tgiorgio/eoe-grs/master/EOE-GRS/eoe.virtualNetworks.parameters.json",
            "contentVersion":"1.0.0.0"
          }
      }
    },
    {
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2018-05-01",
      "name": "Deploy-Fortigate",
      "resourceGroup": "[variables('vnetRG')]",
      "dependsOn": ["Deployt-VNET",
        "[resourceId('Microsoft.Resources/resourceGroups/', variables('vnetRG'))]"
      ],
      "properties": {
          "mode": "Incremental",
          "templateLink": {
              "uri":"https://raw.githubusercontent.com/tgiorgio/eoe-grs/master/Fortigate-HA-ActiveActive-LB/GRS-Fortigate-Template.json"
          },
          "parametersLink": {
            "uri":"https://raw.githubusercontent.com/tgiorgio/eoe-grs/master/EOE-GRS/Fortigate-Template.parameters.json",
            "contentVersion":"1.0.0.0"
          }
      }
    },
    {
      "type": "Microsoft.Resources/resourceGroups",
      "apiVersion": "2018-05-01",
      "location": "[variables('deploylocation')]",
      "name": "[tolower(variables('sharedServicesRG'))]",
      "properties": {}
    },
    {
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2018-05-01",
      "name": "Deploy-StorageAccount",
      "resourceGroup": "[variables('sharedServicesRG')]",
      "dependsOn": [
        "[resourceId('Microsoft.Resources/resourceGroups/',variables('sharedServicesRG'))]"
      ],
      "properties": {
        "mode": "Incremental",
        "templateLink": {
            "uri":"https://raw.githubusercontent.com/tgiorgio/eoe-grs/master/Microsoft.Storage/storage.azuredeploy.json",
            "contentVersion":"1.0.0.0"
          },
        "parametersLink": {
            "uri":"https://raw.githubusercontent.com/tgiorgio/eoe-grs/master/EOE-GRS/eoe.storage.azuredeploy.parameters.json",
            "contentVersion":"1.0.0.0"
          }
      }
    },
    {
      "type": "Microsoft.Resources/resourceGroups",
      "apiVersion": "2018-05-01",
      "location": "[variables('deploylocation')]",
      "name": "[tolower(variables('MgmtRG'))]",
      "properties": {}
    },
    {
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2018-05-01",
      "name": "Deploy-Jumpbox",
      "resourceGroup": "[variables('MgmtRG')]",
      "dependsOn": [
        "[resourceId('Microsoft.Resources/resourceGroups/',variables('MgmtRG'))]",
        "Deploy-StorageAccount",
        "Deployt-VNET"
      ],
      "properties": {
        "mode": "Incremental",
        "templateLink": {
            "uri":"https://raw.githubusercontent.com/tgiorgio/eoe-grs/master/Microsoft.Compute/vm-win-CIS-L1/jumpboxvm.azuredeploy.json",
            "contentVersion":"1.0.0.0"
          },
        "parametersLink": {
            "uri":"https://raw.githubusercontent.com/tgiorgio/eoe-grs/master/EOE-GRS/eoe.jumpboxvm.parameters.json",
            "contentVersion":"1.0.0.0"
          }
      }
    }
    ],
    "outputs": {
    }
}