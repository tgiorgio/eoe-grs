              "customerprefix":{
                "value": "[reference('GRSDeployVNET').outputs.customerprefix.value]"
              },
              "VNetName":{
                "value":"[reference('GRSDeployVNET').outputs.VNetName.value]"
              },
              "vnetResourceGroup" :{
